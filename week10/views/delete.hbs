<h1>{{ title }}</h1> 

<table>
    <tr> <!-- 행 -->
        <td>과목명</td>
        <td>과목번호</td>
    </tr>
    {{#each subject}}  <!-- 데이터 있을 때마다 반복 -->
    <form method="post">
    <tr>
        <td>{{Sname}}</td>
        <td>{{Snumber}}</td>
        <td style="border: none; margin-left: 10px;">
            <button
            style="margin-left: 10px;"
            type="submit" 
            name="delBtn"
            value={{Snumber}}
            formaction="/delete">
            삭제
            </button>
        </td>
    </tr>
    </form>
    {{/each}}
</table>

