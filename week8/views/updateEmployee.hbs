<h1>{{ title }}</h1> <!-- 직원 테이블 갱신 -->

<table>
    <tr> <!-- 행 -->
        <td>Fname</td> <!-- 열 -->
        <td>Minit</td>
        <td>Lname</td>
        <td>Ssn</td>
        <td>Bdate</td>
        <td>Address</td>
        <td>Sex</td>
        <td>Salary</td>
        <td>Super_ssn</td>
        <td>Dno</td>
    </tr>
    {{#each emp_res}}
    <form method="post" action='/update/employee'>
    <tr>
        <td>{{Fname}}</td>
        <td>{{Minit}}</td>
        <td>{{Lname}}</td>
        <td>{{Ssn}}</td>
        <td>{{Bdate}}</td>
        <td>{{Address}}</td>
        <td>{{Sex}}</td>
        <td><input type="text" name="salary" placeholder={{Salary}}></td>
        <td>{{Super_ssn}}</td>
        <td>{{Dno}}</td>
        <td><input type = "submit" value="수정"></td>
    </tr>
    </form>
    {{/each}}
</table>